FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(rho|rhoU|rhoE)"
    {
        solver          diagonal;
        tolerance       1e-10;
        minIter         1;
    }

    "(U|e)"
    {
        solver          smoothSolver;
        smoother        DIC;
        tolerance       1e-8;
        relTol          0;
    }
}


