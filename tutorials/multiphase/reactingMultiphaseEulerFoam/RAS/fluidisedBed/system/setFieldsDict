FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 1
    volScalarFieldValue alpha.particles 0
    volScalarFieldValue alpha.particles1 0
//     volScalarFieldValue alpha.particles2 0
);

regions
(
    cylinderToCell
    {
        p1 (0 0 0);
        p2 (0 0 0.04);
        radius 0.032;

        fieldValues
        (
            volScalarFieldValue alpha.air 0.38
            volScalarFieldValue alpha.particles     0.31
            volScalarFieldValue alpha.particles1    0.31
//             volScalarFieldValue alpha.particles2    0.0
        );
    }
);


