air
{
    type          purePhaseModel;

    diameterModel constant;
    constantCoeffs
    {
        d               1;
    }

    residualAlpha   0;
}

blending
{
    default
    {
        type                linear;
        minFullyContinuousAlpha.air 0;
        minPartlyContinuousAlpha.air 0;
        minFullyContinuousAlpha.particles 1;
        minPartlyContinuousAlpha.particles 1;
        minFullyContinuousAlpha.particles1 1;
        minPartlyContinuousAlpha.particles1 1;
//         minFullyContinuousAlpha.particles2 1;
//         minPartlyContinuousAlpha.particles2 1;
    }
}

