type heatAndMomentumTransferMultiphaseSystem;

phases (particles particles1 air);

particles
{
    type          purePhaseModel;

    diameterModel constant;
    constantCoeffs
    {
        d               120e-6;
    }

    alphaMax        0.63;
    residualAlpha   1e-6;
}

particles1
{
    type          purePhaseModel;

    diameterModel constant;
    constantCoeffs
    {
        d               185e-6;
    }

    alphaMax        0.63;
    residualAlpha   1e-6;
}

