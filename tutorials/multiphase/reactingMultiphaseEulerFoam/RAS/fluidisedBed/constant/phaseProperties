kineticTheory
{
    name                    Particles;
    constantPackingLimit    yes;
    alphaMax                0.63;

    radialModel             Lebowitz;
    frictionalStressModel   JohnsonJackson;
    residualAlpha               1e-4;

    coeffRest
    (
        (particles and particles)   0.99
        (particles and particles1)  0.99
        (particles1 and particles1) 0.99
//         (particles and particles2)  0.99
//         (particles1 and particles2) 0.99
//         (particles2 and particles2) 0.99
    );
    coeffFric
    (
        (particles and particles)   0.1
        (particles and particles1)  0.1
        (particles1 and particles1) 0.1
//         (particles and particles2)  0.1
//         (particles1 and particles2) 0.1
//         (particles2 and particles2) 0.1
    );

    SchaefferCoeffs
    {
        alphaMinFrictionByAlphap 0.99;
        alphaMinFriction        0.5;
        Fr                      0.05;
        eta                     2;
        p                       5;
        phi                     28.5;
        alphaDeltaMin           0.05;
    }

    JohnsonJacksonCoeffs
    {
        alphaMinFriction        0.5;
        Fr                      0.05;
        eta                     2;
        p                       5;
        phi                     28.5;
        alphaDeltaMin           0.05;
    }
}

// Minimum allowable pressure
pMin            10000;


