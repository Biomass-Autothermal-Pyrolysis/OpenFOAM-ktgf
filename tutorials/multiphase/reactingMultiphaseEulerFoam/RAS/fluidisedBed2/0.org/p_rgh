FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [1 -1 -2 0 0 0 0];

internalField       uniform 1e5;

boundaryField
{
    primaryInlet
    {
        type               fixedFluxPressure;
        value              $internalField;
    }
    secondaryInlet
    {
        type               fixedFluxPressure;
        value              $internalField;
    }
    gasOutlet
    {
        type               fixedFluxPressure;//prghPressure;
        p                  $internalField;
        value              $internalField;
    }
    walls
    {
        type               fixedFluxPressure;
        value              $internalField;
    }
    defaultFaces
    {
        type               empty;
    }
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
