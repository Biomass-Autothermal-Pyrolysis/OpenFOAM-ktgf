FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphat.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    primaryInlet
    {
        type            calculated;
        value           $internalField;
    }
    secondaryInlet
    {
        type            calculated;
        value           $internalField;
    }
    gasOutlet
    {
        type            calculated;
        value           $internalField;
    }
    walls
    {
        type            compressible::alphatWallFunction;
        Prt             0.85;
        value           $internalField;
    }
    defaultFaces
    {
        type            empty;
    }
}

