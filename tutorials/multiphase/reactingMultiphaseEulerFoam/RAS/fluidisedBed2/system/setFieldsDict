FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 1
    volScalarFieldValue alpha.particles 0
    volScalarFieldValue alpha.particles1 0
);

regions
(
//     boxToCell
//     {
//         box (0 0 -0.1) (0.03 0.04 0.1);
//         fieldValues
//         (
//             volScalarFieldValue alpha.air 0.38
//             volScalarFieldValue alpha.particles     0.31
//             volScalarFieldValue alpha.particles1    0.31
//         );
//     }

    cylinderToCell
    {
        p1 (0 0 0);
        p2 (0 1 0);
        radius 0.015;

        fieldValues
        (
            volScalarFieldValue alpha.air 0.9752
            volScalarFieldValue alpha.particles     0.0124
            volScalarFieldValue alpha.particles1    0.0124
        );
    }
);


