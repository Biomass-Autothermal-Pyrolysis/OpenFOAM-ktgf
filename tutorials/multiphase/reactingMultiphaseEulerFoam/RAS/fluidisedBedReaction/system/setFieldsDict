FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 1
    volScalarFieldValue alpha.particles1 0
    volScalarFieldValue alpha.particles2 0
    volScalarFieldValue T.air 300.0
    volScalarFieldValue T.particles1 300.0
    volScalarFieldValue T.particles2 300.0
    volScalarFieldValue wood.particles2 1.0
    volScalarFieldValue char.particles2 0
    volScalarFieldValue gas.particles2 0
);

regions
(
    boxToCell
    {
        box (-0.15 0 -1) (0.15 0.5 1);

        fieldValues
        (
            volScalarFieldValue alpha.air 0.4
            volScalarFieldValue alpha.particles1    0.30
            volScalarFieldValue alpha.particles2    0.30
        );
    }
);


